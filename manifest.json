{
  "name": "p5.js Editor Flipper",
  "action": {
    "default_icon": {
      "32": "images/icon_32.png",
      "128": "images/icon_128.png",
      "512": "images/icon_512.png"
    }
  },
  "manifest_version": 3,
  "version": "0.1",
  "description": "Flip panes of p5.js editor",
  "content_scripts": [
    {
      "matches": ["https://editor.p5js.org/*"],
      "js": ["flip.js"]
    }
  ]
}
